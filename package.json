{
  "name": "interfaz-link",
  "version": "1.0.0",
  "description": "Socket para acceder a la interfaz a trav√©s del puerto serie.",
  "main": "index.js",
  "build": {
    "appId": "ar.robotica",
    "nsis": {
      "oneClick": false
    },
    "productName": "Interfaz Link",
    "files": [
      "**/*",
      "resources/",
      "resources/64.png",
      "css/",
      "js/",
      "src/public/",
      "build/interfaz.png"
    ]
  },
  "scripts": {
    "start": "electron .",
    "postinstall": "electron-builder install-app-deps",
    "dist": "electron-builder",
    "install": "electron-rebuild -f -w serialport",
    "pack": "electron-builder --dir",
    "pack-win": "electron-builder build --dir --win ",
    "pack-win32": "electron-builder build --dir --win --ia32",
    "dist-win": "electron-builder -c electron-builder.yaml --windows --x64 ",
    "dist-m1": "electron-builder --mac --universal -c.mac.identity=null",
    "dist-win32": "electron-builder build --win --ia32"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^18.6.4",
    "electron": "^15.0.0",
    "electron-builder": "^22.13.1",
    "electron-rebuild": "^3.2.9",
    "electron-packager": "^15.5.1"
  },
  "dependencies": {
    "color-string": "^1.9.1",
    "fs": "^0.0.1-security",
    "http": "^0.0.1-security",
    "johnny-five": "^2.1.0",
    "materialize-css": "^1.0.0",
    "path": "^0.12.7",
    "serialport": "^10.4.0",
    "socket.io": "^4.5.1",
    "utf8": "^3.0.0"
  }
}
